    <body class="contain">
      <header class="w-100">
        
        <ul class="list-unstyled head-list list-group-horizontal text-center d-flex">
          <li class="dropdown px-1 py-2 "> 
            <a class="btn about-btn py-3" href="/">Home  </a>    
          </li>
          <li class="dropdown px-1 py-2"> 
            <button type="button " class="btn btn-block p-3" data-toggle="dropdown">Blog</button>    
            <div class="dropdown-menu  ">
              <a  href="/new" class="dropdown-item w-75 my-1 ">Add new blog </a>
              <a  href="/campground" class="dropdown-item w-75 my-1 ">All Posts</a>
            </div>
          </li>
          <% if (user) { %>
            <li>Welcome, <%= user.email %></li>
            <li><a href="/logout">Log out</a></li>
          <% } else { %>
            <li><a href="/login">Log in</a></li>
            <li><a href="/signup" class="btn">Sign up</a></li>
          <% } %>
        </ul>
      </header>